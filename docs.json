{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "maple",
  "name": "VCT Docs",
  "colors": {
    "primary": "#9A3412",
    "light": "#EA580C",
    "dark": "#9A3412"
  },
  "favicon": "/favicon.png",
  "icons": {
    "library": "lucide"
  },
  "navigation": {
    "dropdowns": [
      {
        "dropdown": "Documentation",
        "icon": "book",
        "description": "Set up your documentation",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "index",
              "quickstart",
              "development"
            ]
          },
          {
            "group": "Essentials",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/settings",
              "essentials/navigation",
              "essentials/reusable-snippets"
            ]
          }
        ]
      },
      {
        "dropdown": "API Documentation",
        "description": "Reference for the API",
        "icon": "terminal",
        "groups": [
          {
            "group": "API Documentation",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Endpoint Examples",
            "pages": [
              "api-reference/endpoint/get",
              "api-reference/endpoint/create",
              "api-reference/endpoint/delete"
            ]
          }
        ]
      }
    ]
  },
  "navbar": {
    "primary": {
      "type": "button",
      "label": "Get Started", 
      "href": "https://example.com"
    }
  },
  "logo": {
    "light": "/images/logo.png",
    "dark": "/images/logo.png"
  },
  "thumbnails": {
    "background": "/images/embeds/background.png"
  },
  "footer": {
    "socials": {
      "github": "https://github.com/Voiid-Studios/voiidcountdown"
    }
  }
}